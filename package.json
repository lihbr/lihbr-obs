{
  "name": "lihbr-obs",
  "version": "0.0.0",
  "description": "Stream overlays made with Vue.js, crazy uh?",
  "private": true,
  "author": "lihbr <hello@lihbr.com> (https://lihbr.com)",
  "license": "MIT",
  "scripts": {
    "dev": "yarn tailwind && parcel src/*.html --port 3000",
    "generate": "yarn tailwind && parcel build src/*.html",
    "tailwind": "tailwindcss-cli build ./src/assets/css/tailwind.css --config ./tailwind.config.js --output ./src/assets/css/tailwind.build.css"
  },
  "engines": {
    "node": ">=10.16.3",
    "yarn": ">=1.13.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "staticFiles": {
    "staticPath": "static",
    "watcherGlob": "**"
  },
  "dependencies": {
    "@fontsource/antic-slab": "4.2.1",
    "@fontsource/roboto": "4.2.1",
    "assets": "3.0.1",
    "comfy.js": "1.1.6",
    "vue-hot-reload-api": "2.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@vue/component-compiler-utils": "3.2.0",
    "babel-eslint": "10.1.0",
    "color": "3.1.3",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-vue": "7.4.0",
    "husky": "4.3.8",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-static-files-copy": "2.5.1",
    "prettier": "2.2.1",
    "sass": "1.32.7",
    "tailwindcss-cli": "0.1.2",
    "vue": "2.6.12",
    "vue-template-compiler": "2.6.12"
  }
}
